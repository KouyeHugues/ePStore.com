{% extends "frontend_base.html.twig" %}

{% block stylesheets %}
	{{ parent()}}
{% endblock %}

{% block javascripts %}
	{{ parent()}}
{% endblock %}


{# Page Meta title #}
{% set title = "Page non trouvée" %}
{% set description = "Page non trouvée" %}
{% set keywords = "orientesup" %}

{# Page Meta title #}
{% block title %}
	{{title}}
{% endblock %}
{% block fb_title %}
	{{title}}
{% endblock %}
{% block twitter_title %}
	{{title}}
{% endblock %}

{# Page Body class #}
{% block body_class %}{% endblock %}

{# Page meta informations #}
{% block descriptions %}
	{{description}}
{% endblock %}
{% block schema_description %}
	{{description}}
{% endblock %}
{% block twitter_description %}
	{{description}}
{% endblock %}
{% block open_graph_description %}
	{{description}}
{% endblock %}
{% block keywords %}
	{{keywords}}
{% endblock %}

{% block og_image %}
	{{ asset('photo/pc.png')}}
{% endblock %}


{% block content %}
	<div class="content-wrapper">
		<div class="container">
			<div class="row">
				<div class="col-sm-12">
					<div class="card" data-aos="fade-up">
						<div class="card-body text-center">
							<div class="aboutus-wrapper">
								<h1 class="mt-5">
									Page non trouvée
								</h1>
								<p class="font-weight-600 fs-15 mb-5 mt-4">
									Cette page n'est pas trouvée.
								</p>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
{% endblock %}
